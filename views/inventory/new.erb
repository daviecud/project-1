<h1>New Product Form</h1>


<div class="midcontainer">

<form method="post" action="/inventory">
  <label for="name">Name:</label>
    <input id="name" type="text" name="" name="name" />
  <label for="description">Description:</label>
    <input id="description" type="text" name="description">
  <label for="quantity">Quantity:</label>
    <input id="qantity" type="number" name="quantity" />
  <label for="cost">Cost:</label>
    <input id="cost" type="number" name="cost" />
  <label for="sell_price">Sell Price:</label>
    <input id="sell_price" type="number" name="sell_price" />
  <label for="size">Size:</label>
    <input id="size" type="number" name="size" />
  <label for="sport_type">Sport Type:</label>
    <input id="sport_type" type="text" name="sport_type" />

    <input type="submit" value="Add Product"/>
</form>

</div>
